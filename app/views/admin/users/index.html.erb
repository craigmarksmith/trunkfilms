<h1>Users</h1>

<%= link_to "Create user &raquo;", new_admin_user_path %>

<table>
  <tr>
    <th>Username</th>
    <th></th>
  </tr>
<% @users.each do |user| %>
  <tr class="<%= cycle('odd', 'even') %>">
    <td><%= user.username %></td>
    <td>
      <%= link_to "Edit", edit_admin_user_path(user) %>
      <%= link_to "Delete", ['admin',user], :method => 'DELETE', :confirm => "Are you sure?" %>
    </td>
  </tr>
<% end %>
</table>